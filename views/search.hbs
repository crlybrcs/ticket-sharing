<div class="content">
  <h2><span class="yellow">When do you need a ticket?</h2></span>
  <div class="form-wrapper">
    <form class="search-form" action="/availableTickets" method="post">
      <div class="form-container">
        <label for="from">From: </label>
        <input type="date" name="from" id="from" placeholder="FROM">
      </div>
      <div class="form-container">
        <label for="until">Until: </label>
        <input type="date" name="until" id="until" placeholder="UNTIL">
      </div>
      <div class="form-container">
        <label for="zone">Zone: </label>
        <div class="ABbutton">
          <input type="radio" name="zone" value="AB" id="AB" checked="checked" />
          <label for="AB">AB</label>
          <input type="radio" name="zone" value="ABC" id="ABC" />
          <label for="ABC">ABC</label>
        </div>
      </div>
      <button class="myButton" type="submit">GET your TICKET!</button>
    </form>
  </div>
</div>